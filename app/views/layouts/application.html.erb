<!DOCTYPE html>
<html>
  <head>
    <title>BeachApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <!-- Bootstrap -->

        <%= stylesheet_link_tag 'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css', integrity: 'sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u', crossorigin: 'anonymous' %>
        
        <%= stylesheet_link_tag 'bootstrap.min' %>
        <%= stylesheet_link_tag 'bootstrap-toggle.min' %>
        <%= stylesheet_link_tag 'custom_style' %>
        <%= stylesheet_link_tag 'font-awesome.min' %>
        <%= stylesheet_link_tag 'et-line' %>
        <%= stylesheet_link_tag 'hover-min' %>
        <%= stylesheet_link_tag 'animate' %>
        <%= stylesheet_link_tag 'owl.carousel' %>
        <%= stylesheet_link_tag 'owl.theme' %>
        <%= stylesheet_link_tag 'owl.transitions' %>
        <%= stylesheet_link_tag 'lightbox' %>
        <%= stylesheet_link_tag 'flexslider' %>

        <!--master slider-->
        <%= stylesheet_link_tag 'masterslider' %>
        <%= stylesheet_link_tag 'style' %>
        <%= stylesheet_link_tag 'main' %>

        <!--cube portfolio-->        
        <%= stylesheet_link_tag 'cubeportfolio.min' %>

        <!--main css file-->
        <%= stylesheet_link_tag 'cs.style' %>

        <%= stylesheet_link_tag 'application' %>

  <body>

<nav class="navbar navbar-default">
  <div class="container-fluid">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="/">Beach App</a>
    </div>

    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
      <ul class="nav navbar-nav">
        <li class="active"><a href="#">About <span class="sr-only">(current)</span></a></li>
        <li><a href="#">Blog</a></li>
      </ul>
    </div><!-- /.navbar-collapse -->
  </div><!-- /.container-fluid -->
</nav>

    <%= yield %>
  </body>
</html>
